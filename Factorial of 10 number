#include<iostream>
using namespace std;
int factorial(int num)
{
    int result=1,i;
    for(i=1;i<=num;i++)
    {
        result=result*i;
    }
    return result;
}
int main ()
{
    int i;
    for(i=1;i<=10;i++)
    {
        cout <<"Factorial of "<< i <<" is "<< factorial(i)<<endl;
    }
    return 0;
}
